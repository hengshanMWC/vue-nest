{
  "name": "vue-nest",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "init": "pnpm i && npm run prepare",
    "start": "vite",
    "preview": "vite preview",
    "build": "run-p type-check \"build-only {@}\" --",
    "translate": "esno ./scripts/i18nTranslate.ts",
    "commit": "cz",
    "prepare": "husky install",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint:fix": "eslint --fix \"{src,scripts}/**/*.{ts,tsx,js,vue}\" --ignore-path .gitignore",
		"prettier:fix": "prettier --write \"(src|scripts)/**/*.(vue|js|ts)\"",
		"format": "pnpm prettier:fix && pnpm lint:fix",
		"format:servers": "cd ./servers && pnpm format",
		"format:all": "pnpm format && pnpm format:servers"
  },
  "dependencies": {
    "@vicons/ionicons5": "^0.12.0",
    "@vueuse/core": "^10.5.0",
    "axios": "^1.5.1",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.35.0",
    "normalize.css": "^8.0.1",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-i18n": "^9.5.0",
    "vue-router": "^4.2.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@playwright/test": "^1.38.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.3",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^18.17.17",
    "@vitalets/google-translate-api": "^9.2.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "http-proxy-agent": "^7.0.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "npm-run-all2": "^6.0.6",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.2.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.34.4",
    "vue-tsc": "^1.8.11"
  },
  "lint-staged": {
    "**/*.{vue,cjs,mjs,ts,cts,mts}": "pnpm format:all"
  }
}
