{
  "name": "@item/client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "preview": "vite preview",
    "build": "run-p type-check \"build-only {@}\" --",
    "translate": "esno ./scripts/i18nTranslate.ts",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,css,scss,vue,html,md}\"",
    "lint:eslint": "eslint --fix \"{src,scripts,e2e}/**/*.{ts,tsx,js,vue}\" --ignore-path .gitignore",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,postcss,css,scss,html}\"",
    "format": "pnpm lint:prettier && pnpm lint:stylelint && pnpm lint:eslint"
  },
  "dependencies": {
    "@vicons/ionicons5": "^0.12.0",
    "@vueuse/core": "^10.7.0",
    "axios": "^1.6.3",
    "err-code": "^3.0.1",
    "jwt-decode": "^4.0.0",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.36.0",
    "normalize.css": "^8.0.1",
    "pinia": "^2.1.7",
    "vue": "^3.3.13",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@abmao/eslint-config-vue": "^2.1.2",
    "@abmao/stylelint-config-vue": "^0.0.2",
    "@antfu/eslint-config-vue": "^0.43.1",
    "@intlify/unplugin-vue-i18n": "^1.6.0",
    "@playwright/test": "^1.40.1",
    "@rushstack/eslint-patch": "^1.6.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/lodash-es": "^4.17.12",
    "@vitalets/google-translate-api": "^9.2.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.16",
    "http-proxy-agent": "^7.0.0",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.25.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^1.8.27"
  }
}